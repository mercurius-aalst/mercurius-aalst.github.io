
<section class="titled-section" style=@Style>
	<MudText Color="Color.Primary" Typo="Typo.h4" Align="@Alignment" Class="fw-bold">@Title</MudText>
	<div>
		@ChildContent
	</div>
</section>

@code {
	[Parameter] public RenderFragment? ChildContent { get; set; }
	[Parameter] public string Title { get; set; } = default!;
	[Parameter] public Align Alignment { get; set; } = Align.Center;
	[Parameter] public string Style { get; set; } = "";
}

